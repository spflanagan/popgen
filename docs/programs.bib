@article{meffertNonadditiveGeneticEffects2002,
  title = {Nonadditive {{Genetic Effects}} in {{Animal Behavior}}},
  author = {Meffert, Lisa and {xa} and M and Hicks, Sara and {xa} and K and Regan, Jennifer and {xa} and L},
