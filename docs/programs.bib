@article{marionExtendingConceptDiversity2015,
  title = {Extending the {{Concept}} of {{Diversity Partitioning}} to {{Characterize Phenotypic Complexity}}},
  author = {Marion, Zachary H. and Fordyce, James A. and Fitzpatrick, Benjamin M.},
  date = {2015-09},
  journaltitle = {The American Naturalist},
  shortjournal = {The American Naturalist},
  volume = {186},
